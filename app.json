{
  "expo": {
    "name": "7GNETWORK",
    "slug": "7GNETWORK",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "manifest": {
      "name": "MyBeller",
      "short_name": "MyBeller",
      "start_url": "/?utm_source=pwa",
      "display": "standalone",
      "background_color": "#000000",
      "theme_color": "#2563EB",
      "icons": [
        {
          "src": "./assets/images/icon-192.png",
          "sizes": "192x192",
          "type": "image/png"
        },
        {
          "src": "./assets/images/icon-512.png",
          "sizes": "512x512",
          "type": "image/png"
        }
      ]
    }
  }
}